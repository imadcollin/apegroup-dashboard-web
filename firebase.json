{
    "database": {

    },
    "hosting": {
        "public": "build",
        "cleanUrls": false,
        "trailingSlash": false,
        "ignore": [],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=0"
                    }
                ]
            }
        ]
    }
}
